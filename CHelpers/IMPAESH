#ifndef IMPAESH_INCLUDE
#define IMPAESH_INCLUDE
#include <impdat.h>
#include <impcip.h>
int doImportAES (char * pKey, char * pPrivate, char * pType,
    char ** pData, int * lData)
{
  if (*pType == 'D') // a data type key
  return importData(pKey,pPrivate,pType,pData,lData);
  else
  if (*pType == 'C') // a data type key
  return importCipher(pKey,pPrivate,pType,pData,lData);
  else
  {
     printf("Invalid type:needs to be Data or Cipher\n");
     return 8;
  }
}
#endif
